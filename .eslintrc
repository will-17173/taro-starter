{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "Taro" }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx"] }],
    "jsx-quotes": "off",
    "import/no-commonjs": "off",
    "import/no-mutable-exports": "off",
    "import/prefer-default-export": "off",
    "react/sort-comp": [0]
  },
  "parser": "babel-eslint",
  "globals": {
    "wx": false
  }
}
